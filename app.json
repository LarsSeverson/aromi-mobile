{
  "expo": {
    "name": "aromi",
    "slug": "aromi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#CC612B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aromiInc.aromi"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.aromiInc.aromi"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Palanquin-Bold.ttf",
            "./src/assets/fonts/Palanquin-ExtraLight.ttf",
            "./src/assets/fonts/Palanquin-Light.ttf",
            "./src/assets/fonts/Palanquin-Medium.ttf",
            "./src/assets/fonts/Palanquin-Regular.ttf",
            "./src/assets/fonts/Palanquin-SemiBold.ttf",
            "./src/assets/fonts/Palanquin-Thin.ttf",
            "./src/assets/fonts/PalanquinDark-Bold.ttf",
            "./src/assets/fonts/PalanquinDark-Medium.ttf",
            "./src/assets/fonts/PalanquinDark-Regular.ttf",
            "./src/assets/fonts/PalanquinDark-SemiBold.ttf",
            "./src/assets/fonts/RobotoMono-Regular.ttf",
            "./src/assets/fonts/PlaywriteGBS-Regular.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
